        "  model.matrix( ~ 0 + factor(id), data = ex)[,],",
        "+ ",
        "  ex[, s1],",
        "+ ",
        "  mu_beta = rep(0, 13),",
        "+ ",
        "  sigma_beta = diag(c(10^2, rep(1, 12))),",
        "+ ",
        "  mu = rep(0, N + 13),",
        "+ ",
        "  sigma = diag(1, N + 13),",
        "+ ",
        "  Aeps = 1e-2, Beps = 1e-2, Au = 1e-2, Bu = 1e-2",
